{
  "name": "proxmox-mpc",
  "version": "1.0.0",
  "description": "Interactive Infrastructure-as-Code Console for Proxmox VE - Professional virtual machine and container management with automated configuration generation",
  "main": "dist/index.js",
  "bin": {
    "proxmox-mpc": "bin/proxmox-mpc"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/console.ts",
    "console": "tsx src/console.ts",
    "cli": "tsx src/cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:database": "jest --testPathPattern=database --runInBand",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "version:auto": "./scripts/version-bump.sh",
    "version:patch": "./scripts/version-bump.sh --type patch",
    "version:minor": "./scripts/version-bump.sh --type minor",
    "version:major": "./scripts/version-bump.sh --type major",
    "version:prerelease": "npm version prerelease --preid=alpha --no-git-tag-version",
    "version:dry-run": "./scripts/version-bump.sh --dry-run",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "changelog:generate": "./scripts/generate-changelog.sh",
    "changelog:auto": "./scripts/generate-changelog.sh --release-type auto",
    "changelog:major": "./scripts/generate-changelog.sh --release-type major",
    "changelog:minor": "./scripts/generate-changelog.sh --release-type minor",
    "changelog:patch": "./scripts/generate-changelog.sh --release-type patch",
    "changelog:prerelease": "./scripts/generate-changelog.sh --release-type prerelease",
    "changelog:dry-run": "./scripts/generate-changelog.sh --dry-run --verbose",
    "changelog:validate": "./scripts/validate-changelog.sh",
    "changelog:validate:fix": "./scripts/validate-changelog.sh --fix",
    "release": "./scripts/release.sh --type auto",
    "release:auto": "./scripts/release.sh --type auto",
    "release:patch": "./scripts/release.sh --type patch",
    "release:minor": "./scripts/release.sh --type minor",
    "release:major": "./scripts/release.sh --type major",
    "release:prerelease": "./scripts/release.sh --type prerelease",
    "release:dry-run": "./scripts/release.sh --dry-run",
    "release:prepare": "npm run build && npm run test && npm run lint && npm run typecheck",
    "release:validate": "./scripts/validate-release.sh",
    "release:publish": "npm publish --access public",
    "prepublishOnly": "npm run release:prepare",
    "postversion": "git push && git push --tags",
    "prepare": "husky",
    "validate:pre-release": "./scripts/pre-release-validation.sh",
    "validate:checklist": "./scripts/validation-checklist.sh",
    "validate:checklist:interactive": "./scripts/validation-checklist.sh --interactive",
    "validate:checklist:auto-fix": "./scripts/validation-checklist.sh --auto-fix --interactive",
    "test:environment:setup": "./scripts/setup-test-environment.sh",
    "test:environment:staging": "./scripts/setup-test-environment.sh --environment=staging",
    "test:environment:production": "./scripts/setup-test-environment.sh --environment=production",
    "test:release": "./scripts/run-release-tests.sh",
    "test:release:staging": "./scripts/run-release-tests.sh --environment=staging --comprehensive",
    "test:release:production": "./scripts/run-release-tests.sh --environment=production --comprehensive",
    "verify:release": "./scripts/release-verification.sh",
    "verify:release:comprehensive": "./scripts/release-verification.sh --verification-level=comprehensive",
    "monitor:post-release": "./scripts/post-release-monitoring.sh",
    "rollback:system": "./scripts/rollback-system.sh",
    "rollback:npm": "./scripts/rollback-system.sh --rollback-type=npm",
    "rollback:git": "./scripts/rollback-system.sh --rollback-type=git",
    "validate:distribution": "./scripts/validate-package-distribution.sh",
    "validate:distribution:comprehensive": "./scripts/validate-package-distribution.sh --comprehensive",
    "qa:dry-run": "./scripts/release-dry-run.sh",
    "qa:dry-run:comprehensive": "./scripts/release-dry-run.sh --comprehensive",
    "qa:dry-run:rc": "./scripts/release-dry-run.sh --version=1.0.0-rc.1 --comprehensive"
  },
  "keywords": [
    "proxmox",
    "infrastructure",
    "iac",
    "virtualization",
    "cli",
    "console",
    "homelab",
    "terraform",
    "ansible",
    "automation"
  ],
  "author": {
    "name": "Proxmox-MPC Team",
    "email": "contact@proxmox-mpc.dev"
  },
  "homepage": "https://proxmox-mpc.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/proxmox-mpc/proxmox-mpc.git"
  },
  "bugs": {
    "url": "https://github.com/proxmox-mpc/proxmox-mpc/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "files": [
    "dist/**/*",
    "bin/**/*",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "ink-testing-library": "^4.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.2.5",
    "semver": "^7.7.2",
    "standard-version": "^9.5.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@types/js-yaml": "^4.0.9",
    "@types/react": "^19.1.12",
    "@types/uuid": "^10.0.0",
    "axios": "^1.6.7",
    "commander": "^12.0.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "fuse.js": "^7.1.0",
    "ink": "^6.2.3",
    "ink-ui": "^0.4.0",
    "js-yaml": "^4.1.0",
    "prisma": "^5.22.0",
    "react": "^19.1.1",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  }
}
